{{ form_start(form) }}

  <div class="my-2">
    {{ form_row(form.title) }}
  </div>

  <div class="my-2">
    {{ form_row(form.content) }}
  </div>

  <div class="my-2">
    {{ form_row(form.igVersion) }}
  </div>

  <div class="my-2">
    {{ form_row(form.igDate) }}
